<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="php">
                    <match url="^(.+)$" />
                    <action type="Rewrite" url="index.php?url={R:0}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
